{
  "o3": {
    "color": {
      "use-case": {
        "link": {
          "text": {
            "@": {
              "description": "",
              "$type": "color",
              "$value": "{o3.color.palette.teal}"
            },
            "hover": {
              "$type": "color",
              "$value": "{o3.color.palette.slate}"
            }
          },
          "underline": {
            "@": {
              "$type": "color",
              "$value": "{o3.color.palette.teal}"
            },
            "hover": {
              "$type": "color",
              "$value": "{o3.color.palette.slate}"
            }
          }
        },
        "page": {
          "background": {
            "$type": "color",
            "$value": "{o3.color.palette.white}"
          }
        },
        "body": {
          "text": {
            "$type": "color",
            "$value": "{o3.color.palette.black-90}"
          }
        },
        "muted": {
          "text": {
            "$type": "color",
            "$value": "{o3.color.palette.black-50}",
            "$description": "\"Muted\" text is less prominent, for example credits and captions."
          }
        },
        "support": {
          "text": {
            "$type": "color",
            "$value": "{o3.color.palette.black-70}"
          }
        },
        "support-inverse": {
          "text": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "mix",
                  "value": "0.1",
                  "space": "srgb",
                  "color": "{o3.color.palette.slate} "
                }
              }
            },
            "$type": "color",
            "$value": "{o3.color.palette.white}"
          }
        },
        "muted-inverse": {
          "text": {
            "$type": "color",
            "$value": "#a8aaad",
            "$description": "\"Muted\" text is less prominent, for example credits and captions."
          }
        },
        "link-inverse": {
          "text": {
            "@": {
              "$type": "color",
              "$value": "#ffffffff"
            },
            "hover": {
              "$type": "color",
              "$value": "#d4d4d6"
            }
          },
          "underline": {
            "@": {
              "$type": "color",
              "$value": "#ffffffff"
            },
            "hover": {
              "$type": "color",
              "$value": "#d4d4d6"
            }
          }
        },
        "page-inverse": {
          "background": {
            "$type": "color",
            "$value": "{o3.color.palette.slate}"
          }
        },
        "body-inverse": {
          "text": {
            "$type": "color",
            "$value": "{o3.color.palette.white}"
          }
        },
        "button": {
          "foreground": {
            "$type": "color",
            "$value": "{o3.color.palette.white}"
          },
          "foreground-disabled": {
            "$type": "color",
            "$value": "{o3.color.palette.white}"
          },
          "default": {
            "$type": "color",
            "$value": "{o3.color.palette.teal}"
          },
          "hover": {
            "$type": "color",
            "$value": "{o3.color.palette.teal-40}"
          },
          "pressed": {
            "$type": "color",
            "$value": "{o3.color.palette.teal-20}"
          },
          "disabled": {
            "$type": "color",
            "$value": "#9EC0BD"
          }
        },
        "error-background": {
          "$type": "color",
          "$value": "rgba(204,0,0,0.06)"
        },
        "error-text": {
          "$type": "color",
          "$value": "{o3.color.palette.crimson}"
        },
        "error": {
          "$type": "color",
          "$value": "{o3.color.palette.crimson}",
          "$description": "[DEPRECATED] This token is going to be looked at in the upcoming audit."
        },
        "success-background": {
          "$type": "color",
          "$value": "#D7F0D1"
        },
        "success-foreground": {
          "$type": "color",
          "$value": "#00572C"
        }
      }
    }
  }
}