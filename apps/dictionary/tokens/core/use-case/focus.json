{
  "o3": {
    "focus": {
      "use-case": {
        "ring": {
          "inner": {
            "$type": "boxShadow",
            "$value": {
              "x": "0",
              "y": "0",
              "blur": "0",
              "spread": "4",
              "color": "{o3.color.palette.white}",
              "type": "dropShadow"
            }
          },
          "outer": {
            "$type": "boxShadow",
            "$value": {
              "x": "0",
              "y": "0",
              "blur": "0",
              "spread": "8",
              "color": "{o3.color.palette.black-70}",
              "type": "dropShadow"
            }
          },
          "inverse": {
            "inner": {
              "$type": "boxShadow",
              "$value": {
                "x": "0",
                "y": "0",
                "blur": "0",
                "spread": "4",
                "color": "{o3.color.palette.black-70}",
                "type": "dropShadow"
              }
            },
            "outer": {
              "$type": "boxShadow",
              "$value": {
                "x": "0",
                "y": "0",
                "blur": "0",
                "spread": "8",
                "color": "{o3.color.palette.white}",
                "type": "dropShadow"
              }
            }
          }
        },
        "outline": {
          "color": {
            "$type": "boxShadow",
            "$value": {
              "x": "0",
              "y": "0",
              "blur": "0",
              "spread": "2",
              "color": "{o3.color.palette.black-50}",
              "type": "dropShadow"
            }
          },
          "inverse": {
            "color": {
              "$type": "boxShadow",
              "$value": {
                "x": "0",
                "y": "0",
                "blur": "0",
                "spread": "2",
                "color": "{o3.color.palette.white}",
                "type": "dropShadow"
              }
            }
          }
        }
      }
    }
  }
}