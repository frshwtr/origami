{
  "o3": {
    "editorial-typography": {
      "big-number-title": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{o3.font.family.metric}",
          "fontWeight": "{o3.font.weight.semibold}",
          "fontSize": "{o3.font.size-metric2.7}",
          "lineHeight": "{o3.font.lineheight-metric2.7}"
        }
      },
      "pullquote-content": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{o3.font.family.metric}",
          "fontWeight": "{o3.font.weight.semibold}",
          "fontSize": "{o3.font.size-metric2.2}",
          "lineHeight": "{o3.font.lineheight-metric2.2}"
        }
      },
      "topic-tag": {
        "color": {
          "$type": "color",
          "$value": "{o3.color.palette.black-80}"
        },
        "inverse-color": {
          "$type": "color",
          "$value": "{o3.color.palette.white}"
        },
        "hover": {
          "color": {
            "$type": "color",
            "$value": "{o3.color.palette.black}"
          },
          "inverse-color": {
            "$type": "color",
            "$value": "{o3.color.use-case.link-inverse.text.hover}"
          }
        }
      },
      "caption": {
        "color": {
          "$type": "color",
          "$value": "{o3.color.palette.black-70}"
        },
        "inverse-color": {
          "$type": "color",
          "$value": "{o3.color.use-case.muted-inverse.text}"
        }
      },
      "byline-author": {
        "color": {
          "$type": "color",
          "$value": "{o3.color.palette.black-80}"
        },
        "inverse-color": {
          "$type": "color",
          "$value": "{o3.color.palette.white}"
        },
        "hover": {
          "color": {
            "$type": "color",
            "$value": "{o3.color.palette.black}"
          },
          "inverse-color": {
            "$type": "color",
            "$value": "{o3.color.use-case.link-inverse.text.hover}"
          }
        }
      },
      "byline-timestamp": {
        "color": {
          "$type": "color",
          "$value": "{o3.color.palette.black-60}"
        },
        "inverse-color": {
          "$type": "color",
          "$value": "{o3.color.use-case.muted-inverse.text}"
        }
      }
    }
  }
}