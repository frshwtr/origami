{
  "o3": {
    "color": {
      "use-case": {
        "link": {
          "text": {
            "@": {
              "description": "",
              "$type": "color",
              "$value": "#757575"
            },
            "hover": {
              "$type": "color",
              "$value": "{o3.color.palette.black-60}"
            }
          },
          "underline": {
            "@": {
              "$type": "color",
              "$value": "{o3.color.palette.black-30}"
            },
            "hover": {
              "$type": "color",
              "$value": "{o3.color.palette.black-70}"
            }
          }
        },
        "link-inverse": {
          "text": {
            "@": {
              "$type": "color",
              "$value": "{o3.color.palette.white}"
            },
            "hover": {
              "$type": "color",
              "$value": "{o3.color.palette.black-20}"
            }
          },
          "underline": {
            "@": {
              "$type": "color",
              "$value": "{o3.color.palette.white}"
            },
            "hover": {
              "$type": "color",
              "$value": "{o3.color.palette.black-20}"
            }
          }
        },
        "page": {
          "background": {
            "$type": "color",
            "$value": "{o3.color.palette.white}"
          }
        },
        "page-inverse": {
          "background": {
            "$type": "color",
            "$value": "{o3.color.palette.black-80}"
          }
        },
        "body": {
          "text": {
            "$type": "color",
            "$value": "{o3.color.palette.black-80}"
          }
        },
        "support": {
          "text": {
            "$type": "color",
            "$value": "{o3.color.palette.black-70}"
          }
        },
        "support-inverse": {
          "text": {
            "$type": "color",
            "$value": "{o3.color.palette.black-10}"
          }
        },
        "body-inverse": {
          "text": {
            "$type": "color",
            "$value": "{o3.color.palette.white}"
          }
        },
        "muted": {
          "text": {
            "$type": "color",
            "$value": "#757575",
            "$description": "\"Muted\" text is less prominent, for example credits and captions."
          }
        },
        "muted-inverse": {
          "text": {
            "$type": "color",
            "$value": "{o3.color.palette.black-20}"
          }
        },
        "heading": {
          "text": {
            "$type": "color",
            "$value": "{o3.color.palette.black-90}"
          }
        },
        "heading-inverse": {
          "text": {
            "$type": "color",
            "$value": "{o3.color.palette.white}"
          }
        },
        "button": {
          "foreground": {
            "$type": "color",
            "$value": "{o3.color.palette.white}"
          },
          "foreground-disabled": {
            "$type": "color",
            "$value": "{o3.color.palette.white}"
          },
          "default": {
            "$type": "color",
            "$value": "{o3.color.palette.black-90}"
          },
          "hover": {
            "$type": "color",
            "$value": "{o3.color.palette.black-80}"
          },
          "pressed": {
            "$type": "color",
            "$value": "{o3.color.palette.black-70}"
          },
          "disabled": {
            "$type": "color",
            "$value": "{o3.color.palette.black-20}"
          }
        },
        "error-background": {
          "$type": "color",
          "$value": "rgba(204,0,0,0.06)"
        },
        "error-text": {
          "$type": "color",
          "$value": "{o3.color.palette.crimson}"
        },
        "error": {
          "$type": "color",
          "$value": "{o3.color.palette.crimson}",
          "$description": "[DEPRECATED] This token is going to be looked at in the upcoming audit."
        },
        "success-background": {
          "$type": "color",
          "$value": "#D7F0D1"
        },
        "success-foreground": {
          "$type": "color",
          "$value": "#00572C"
        }
      }
    }
  }
}